{
  "name": "ecsy-three",
  "version": "0.0.4",
  "description": "ECSY three.js bindings",
  "main": "./lib/index.js",
  "module": "./index.js",
  "scripts": {
    "build": "rollup -c",
    "build:cjs": "rimraf lib && babel src -d lib",
    "dev": "concurrently --names 'ROLLUP,DOCS,HTTP' -c 'bgBlue.bold,bgYellow.bold,bgGreen.bold' 'rollup -c -w -m inline' 'npm run dev:server'",
    "dev:server": "http-server -c-1 -p 9090 --cors",
    "start": "npm run dev",
    "lint": "eslint src",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-export-namespace-from": "^7.10.1",
    "@babel/plugin-transform-modules-commonjs": "^7.10.1",
    "@rollup/plugin-json": "^4.0.2",
    "@rollup/plugin-node-resolve": "^6.1.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^5.0.0",
    "ecsy": "git+https://github.com/MozillaReality/ecsy.git",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gh-pages": "^2.1.1",
    "http-server": "^0.12.3",
    "np": "^6.2.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "rollup": "^1.27.13",
    "rollup-plugin-terser": "^6.1.0",
    "three": "^0.116.1",
    "troika-3d-text": "^0.17.0",
    "typedoc": "^0.17.7",
    "typedoc-plugin-markdown": "^2.3.1",
    "typescript": "^3.9.5"
  },
  "peerDependencies": {
    "ecsy": "*",
    "three": "*",
    "troika-3d-text": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mozillareality/ecsy-three.git"
  },
  "author": "Fernando Serrano <fernandojsg@gmail.com> (http://fernandojsg.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mozillareality/ecsy-three/issues"
  },
  "homepage": "https://github.com/mozillareality/ecsy-three#readme"
}
